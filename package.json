{
  "name": "point-card-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prebuild": "yarn global:cli && yarn db",
    "global:cli": "yarn global add ts-node",
    "db": "yarn prisma migrate deploy && yarn prisma generate && yarn prisma:seed",
    "prisma:seed": "yarn prisma db seed",
    "prisma:seed:local": "dotenv -e .env.local -- yarn prisma db seed",
    "prisma:push:local": "yarn prisma format && yarn prisma generate && dotenv -e .env.local -- yarn prisma db push"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/x-date-pickers": "^6.11.1",
    "@prisma/client": "^5.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.16",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "next": "13.4.16",
    "pg": "^8.11.2",
    "postcss": "8.4.28",
    "prisma": "^5.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "prisma": {
    "schema": "utils/server/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} utils/server/prisma/seed.ts"
  }
}
